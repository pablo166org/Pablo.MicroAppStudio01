{
  "ConnectionStrings": {
    {{~ if config.database_provider == "ef" ~}}
    "Administration": "Administration_Database",
    "AbpBlobStoring": "BlobStoring_Database",
    "MicroserviceName": "Server=localhost,1434; User Id=sa; Password=myPassw@rd; Database=MicroAppStudio01_MicroserviceName; TrustServerCertificate=true"
    {{~ else if config.database_provider == "mongodb" ~}}
    "Administration": "Administration_Database",
    "AbpBlobStoring": "BlobStoring_Database",
    "MicroserviceName": "mongodb://localhost:27017/MicroAppStudio01_MicroserviceName"
    {{~ end~}}
  },
  "App": {
    "CorsOrigins": "http://localhost:webgateway_port",
    "EnablePII": false
  },
  "Swagger": {
    "IsEnabled": true
  },
  "AuthServer": {
    "Authority": "http://localhost:authserver_port",
    "MetaAddress": "http://localhost:authserver_port",
    "RequireHttpsMetadata": "false",
    "SwaggerClientId": "SwaggerTestUI",
    "Audience": "MicroserviceName"
  },
  "Redis": {
    "Configuration": "localhost:6379"
  },
  "RabbitMQ": {
    "Connections": {
      "Default": {
        "HostName": "localhost"
      }
    },
    "EventBus": {
      "ClientName": "MicroAppStudio01_MicroserviceName",
      "ExchangeName": "MicroAppStudio01"
    }
  },
  "AbpDistributedCache": {
    "KeyPrefix": "MicroAppStudio01:"
  },
  "DataProtection": {
    "ApplicationName": "MicroAppStudio01",
    "Keys": "MicroAppStudio01-Protection-Keys"
  },
  "ElasticSearch": {
    "IsLoggingEnabled": "true",
    "Url": "http://localhost:9200"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "string_encrryption_key"
  }
}